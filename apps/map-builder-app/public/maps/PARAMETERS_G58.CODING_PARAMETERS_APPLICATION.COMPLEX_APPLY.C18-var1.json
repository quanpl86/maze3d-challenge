  "translations": {
    "vi": {
      "Challenge.PARAMETERS_G58.CODING_PARAMETERS_APPLICATION.COMPLEX_APPLY.C18.Title": "Vẽ Hình chữ nhật",
      "Challenge.PARAMETERS_G58.CODING_PARAMETERS_APPLICATION.COMPLEX_APPLY.C18.Description": "Hãy tạo một hàm `trace_rectangle(width, height)` có tham số. Sử dụng nó để thu thập các viên pha lê được đặt trên chu vi của hai hình chữ nhật có kích thước khác nhau: một hình 3x5 và một hình 6x2.",
      "topic-title-coding_parameters_application": "Áp dụng Tham số"
    },
    "en": {
      "Challenge.PARAMETERS_G58.CODING_PARAMETERS_APPLICATION.COMPLEX_APPLY.C18.Title": "Drawing Rectangles",
      "Challenge.PARAMETERS_G58.CODING_PARAMETERS_APPLICATION.COMPLEX_APPLY.C18.Description": "Create a function `trace_rectangle(width, height)` that takes parameters. Use it to collect crystals placed on the perimeter of two different-sized rectangles: one 3x5 and one 6x2.",
      "topic-title-coding_parameters_application": "Áp dụng Tham số"
    }
  },
